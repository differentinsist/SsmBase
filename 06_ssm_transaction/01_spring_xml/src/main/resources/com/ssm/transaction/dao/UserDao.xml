<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.ssm.transaction.dao.UserDao">
    <select id="queryAll" resultType="com.ssm.transaction.domain.Department">
        select * from department
    </select>

    <select id="findById" resultType="com.ssm.transaction.domain.Department">
        select * FROM department WHERE id = #{id}
    </select>
</mapper>